<header>
    <ul>
        <li><%= link_to 'Home', '/houses' %></li>
        <li><%#= link_to 'User', users_path(current_user.id) %></li>
        <li><%#=# link_to 'Logout', users_path(current_user.id) %></li>
        
        
    </ul>
</header>
<div class="user_new">
<%= form_with(model: @user, local: true) do |f| %>  
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Create my account" %>
<% end %>
</div>